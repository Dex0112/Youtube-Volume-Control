{
    "manifest_version": 2,
    "name": "Youtube Volume Control",
    "version": "1.0",
    "description": "Allows you to precisely set the volume of YouTube videos!",
    "icons": { "42": "icon/volumeControl.png" },
    "content_scripts": [
        {
           "matches": ["*://www.youtube.com/watch*"],
            "js": ["volumeControl.js"]
        }
    ]
}
