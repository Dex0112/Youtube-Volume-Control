{
    "manifest_version": 2,
    "name": "Youtube Volume Control",
    "version": "1.0",
    "description": "Allows you to precisely set the volume of YouTube videos!",
    "icons": { "42": "icon/volumeControl.png" },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "js": ["volumeControl.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icons/PopupIcon.png",
        "default_title": "Volume Control",
        "default_popup": "popup.html"
    }
}
